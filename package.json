{
  "name": "swagular",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve examples",
    "build": "ng build swagular --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "clean": "rm -rf dist/ && rm -rf coverage/",
    "copy-files": "cp -r ./templates ./dist/swagular && cp ./README.md ./dist/swagular ",
    "patch": "cd ./projects/swagular && npm version patch &&  cd ../../",
    "publishh": "npm run patch &&  npm run build && npm run copy-files && cd ./dist/swagular && npm publish && cd ../../  && npm run clean",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.3",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~12.2.3",
    "@angular/compiler": "~12.2.3",
    "@angular/core": "~12.2.3",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~12.2.3",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~12.2.3",
    "@angular/platform-browser-dynamic": "~12.2.3",
    "@angular/router": "~12.2.3",
    "ajv": "7.0.3",
    "ajv-formats": "1.5.1",
    "angular-typesafe-reactive-forms-helper": "^2.0.1",
    "lodash-es": "^4.17.20",
    "ng-table-virtual-scroll": "^1.3.8",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.3",
    "@angular/cli": "~12.2.3",
    "@angular/compiler-cli": "~12.2.3",
    "@types/jasmine": "~3.6.0",
    "@types/lodash-es": "^4.17.4",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.1",
    "prettier": "^2.3.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
